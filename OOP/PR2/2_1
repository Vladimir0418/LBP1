#include "pch.h"
#include <iostream>
#include <string>
#include <math.h>
using namespace std;
int main()
{

	float a, b,n,m;
	setlocale(LC_ALL, "Russian");
	a = 4.8;
	b = -7.9;
	if (a >= b)
	{
		n = a - b;
		n = pow(n, 1.0 / 3);
	}
	n = (a*a) + ((a - b) / (sin(a*b)));
	if (n < b)
	{
		m = (sin(a)*sin(a))*(-cos(n));
		m=((n+a)/(-b))+ pow(m,0.5);
	}
	else
		if (n = b)
		{
			m = (pow(b, 2)) + (tan(n*a));
	    }
	m = (pow(b, 3))+( n * (pow(a,2)));
	cout << a << " " << b << " " << n << " " << m;


	return 0;
}


